{
  "name": "iptv-checker",
  "description": "for chrome extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking", 
		"storage"
  ],
  "icons":
	{
		"16": "images/icon.png",
		"48": "images/icon.png",
		"128": "images/icon.png"
	},
  "browser_action": {
    "default_icon" : "images/icon.png",
    "default_title": "iptv-checker",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "homepage_url": "https://github.com/zhimin-dev/iptv-checker"
}
